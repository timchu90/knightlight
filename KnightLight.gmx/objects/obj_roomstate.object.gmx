            <string>//if all but one playing character has died, declare game over
if(deathcount == (numberofplayers - 1)){
    for(var i =1;i&lt;=4;i++){
        if(menustart[i] == true and playerhp[i] &gt; 0){
            show_message("PLAYER " +string(i)+" WINS!");
            numberOfRounds++;
            /*
                Creates a file and saves the weapon of the winner.
                The file is saved in C:/Users/(Your username)/AppData/Local/KnightLight
            */
            file = file_text_open_append(working_directory + "listOfWinningWeapons.txt");
            if (menuselect[i] == 1)
            {
                file_text_write_string(file, "Flail
");
            }
            else if (menuselect[i] == 2)
            {
                file_text_write_string(file, "Sword
");
            }
            else if (menuselect[i] == 3)
            {
                file_text_write_string(file, "Bow
");
            }
            file_text_close(file);
            if (numberOfRounds == 3)
                room_goto(room_first);
            else
                room_restart();
        }
    }
}

