            <string>//contains code for moving player and turning on/off light
if(deathbool == false){
    if (alarm[4] == -1) MovePlayer(2,obj_reticle2); 
}
if(alarm[4]!= -1&amp;&amp; alarm[4]%2 == 0){
    with(instance_create(x,y,obj_smoke)){
        image_angle = obj_player2.direction-90;
    }
}
CollisionCheck();
if(flashon[2] == false || playerhp[2] &gt;= 200 || deathbool == true ||healrate ==0){
    if(healing == true){
        healing = false;
        with(healID){
            instance_destroy();
        }
    }
}
if(flashon[2] == true &amp;&amp; playerhp[2] &lt; 200 &amp;&amp; deathbool == false&amp;&amp; healrate&gt;0){
    if(healing == false){
        healing = true;
        healID = instance_create(x,y,obj_heal);
    }
    healID.x = x;
    healID.y = y;
    playerhp[2] += healrate;
}
if(playerhp[2] &lt;= 0 &amp;&amp; deathbool == false){
    deathbool = true;
    deathcount++;
    if(menuselect[2] == 1){
        with(obj_link_2_1){
            instance_destroy();
        }   
        with(obj_link_2_2){
            instance_destroy();
        }   
        with(obj_link_2_3){
            instance_destroy();
        }   
        with(obj_flail2){
            instance_destroy();
        }   
    }
    if(menuselect[2] == 2){
        with(obj_hand2){
            instance_destroy();
        }
        with(obj_sword2){
            instance_destroy();
        }
    }
    if(menuselect[2] == 3){
        with(obj_bow2){
            instance_destroy();
        }
    }
    else if(menuselect[2] == 4){
        with(obj_lance2){
            instance_destroy();
        }
    }

    with(obj_reticle2){
        instance_destroy();
    }

    speed = 0;
    image_speed = 1;
    flashon[2] = true;
    alarm[5] = room_speed;

}

</string>
